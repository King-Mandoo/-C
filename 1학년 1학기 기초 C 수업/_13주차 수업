#include<stdio.h>

void dan(int num)
{
	int result;

	for (int i = 1; i < 10; i++)
	{
		result = num * i;
		printf("%2d X %2d = %2d\n", num, i, result);
	}
}

int main()
{
	int num;

	printf("ì¶œë ¥í•˜ê³  ì‹¶ì€ ë‹¨ì„ ì…ë ¥ : ");
	scanf("%d", &num);

	dan(num);

	return 0;
}
