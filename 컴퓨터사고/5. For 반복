# for 문

color = ["red", "green", "blue"]

for i in color:
    print(i)


---------------------------

color = ["red", len("red")]

for i in color:
    print(i)

-----------------------------

num = [1, 2, 3, 4, 5]

for i in num:
    print(i + 1)
    

#---------문제 풀이-----------------------

# 1번.

meal = ['아침', '점심', '저녁', '야식']

date = 1

while date != 0:
    date = int(input("종료(0) 며칠이 지났습니까 : "))
    for i in meal:
        print(date * i)
        
        
🔻🔻🔻 다른 답

meal = ['아침', '점심', '저녁', '야식']

date = 1

i = 0

while date > 0:
    date = int(input("종료(0) 며칠이 지났습니까 : "))

    i = date
    while i > 0:
        for j in meal:
              print(j)
        i = i - 1
    



# 2번.

print("2020 제2회 한국사 시험 결과")

score = [71, 70, 70, 70, 65, 40]

for i in score:
    if i >= 70:
        print("A")
    elif i >= 60:
        print("B")
    elif i < 60:
        print("F")



# 3번.

n = ["토미", "마이크", "불독", "허호"]
c = ["mt", "concert", "play", "go"]

for i in n:
    for j in c:
        print(i,j)

----------------------------------


# range 함수

for a in range(6):   # range에 0부터 5까지 6숫자가 담겨져 있음.
    print(a)

for b in range(3,8): # 0 부터 7까지의 배열 안에서 3부터 7까지 출력
    print (b)

for c in range(3,8,2): # 0부터 7까지의 배열 안에서 3부터 7까지 2씩 띄어서 출력
    print (c)
    
------------

a = int(input("홀수는 1, 짝수는 2를 입력해 주세요: "))

for i in range(a, 101, 2):
    print(i)


