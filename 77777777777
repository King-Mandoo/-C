#include <stdio.h>

int main(void)
{
	int Byear;
	int Syear;

	scanf("%d", &Byear);
	Syear = Byear - 543;
	printf("%d", Syear);

	return 0;
}

-------------------------------------------

#include <stdio.h>

int main(void)
{
	int A, B, C;
	scanf("%d %d %d", &A, &B, &C);
	printf("%d\n", (A + B) % C);
	printf("%d\n", ((A % C) + (B % C)) % C);
	printf("%d\n", (A * B) % C);
	printf("%d\n", ((A % B) * (B % C)) % C);
	return 0;
}

-------------------------------------------
