#include <stdio.h>

int main()
{
    int i = 10;
    int* p;
    p = &i;
    printf("*p = %d\n", *p);
    printf("i = %d\n", i);
    *p = 20;
    printf("*p = %d\n", *p);
    printf("i = %d\n", i);

    return 0;
}

