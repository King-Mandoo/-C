meal = ['ì•„ì¹¨', 'ì ì‹¬', 'ì €ë…', 'ì•¼ì‹']

date = 1

i = 0

while date > 0:
    date = int(input("ì¢…ë£Œ(0) ë©°ì¹ ì´ ì§€ë‚¬ìŠµë‹ˆê¹Œ : "))

    i = date
    while i > 0:
        for j in meal:
              print(j)
        i = i - 1
    



