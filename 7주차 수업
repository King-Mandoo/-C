#include <stdio.h>

int main(void)
{
	int dan;
	int i;

	printf("ëª‡ ë‹¨ ? ");
	scanf("%d", &dan);

	for (i = 9; i > 0; i--)
	{
		printf("%d X %d = %d\n", dan, i, dan * i);
	}

	return 0;
}

