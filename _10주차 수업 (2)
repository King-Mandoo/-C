#include <stdio.h>
 
int main()
{
	char ch;
	char* p;
	char* q;

	ch = 'A';   --> λ¬Έμν• λ³€μ chμ— 'A' λ€μ…
	p = &ch;    --> λ³€μ pμ— ch μ£Όμ† λ€μ…

	q = p;      --> λ³€μ qμ— pμ κ°’ λ€μ…

	*q = 'Z';   --> λ³€μ qκ°€ κ°€λ¥΄ν‚¤λ” μ£Όμ†μ— 'Z'λ€μ…,  λ³€μ qκ°€ κ°€λ¥΄ν‚¤λ” μ£Όμ†κ°€ λ¬Έμν• λ³€μ chμ μ£Όμ†μ΄λ―€λ΅ λ³€μ chμ— 'Z'λ€μ…

	printf("ch => %c", ch);  --> 'Z' 

	return 0;
}					

