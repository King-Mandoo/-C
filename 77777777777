#include <stdio.h>

int main(void)
{
	int Byear;
	int Syear;

	scanf("%d", &Byear);
	Syear = Byear - 543;
	printf("%d", Syear);

	return 0;
}

-------------------------------------------

#include <stdio.h>

int main(void)
{
	int A, B, C;
	scanf("%d %d %d", &A, &B, &C);
	printf("%d\n", (A + B) % C);
	printf("%d\n", ((A % C) + (B % C)) % C);
	printf("%d\n", (A * B) % C);
	printf("%d\n", ((A % B) * (B % C)) % C);
	return 0;
}

-------------------------------------------

#include <stdio.h>

int main(void)
{
	int Hg, bit, channel, sec;
	float CD;
	scanf("%d %d %d %d", &Hg, &bit, &channel, &sec);

	CD = ((float)Hg * bit * channel * sec) / 8;
	printf("%.1fMB", CD / 1024 / 1024);

	return 0;
}
