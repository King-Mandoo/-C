 #include <stdio.h>

int main()
{
	char stack[5];
	int top = 0;

	char carName = 'A';
	int select = 9;

	while (select != 3)
	{
		printf("<1> μ°¨ μ¶”κ°€ <2> μ°¨ λΉΌκΈ° <3> λ : ");
		scanf("%d", &select);

		switch (select)
		{
		case 1: 
			if (top >= 5)
			{
				printf("λ” μ΄μƒ μ¶”κ°€ λ¶κ°€λ¥!\n");
			}
			else
			{
				stack[top] = carName++;
				printf("%cμ°¨ μ¶”κ°€!\n", stack[top]);
				top++;
			}
			break;

		case 2:
			if (top <= 0)
			{
				printf("λ” μ΄μƒ λΉΌκΈ° λ¶κ°€λ¥!\n");
			}
			else
			{
				top--;
				printf("%cμ°¨ λΉΌκΈ°!\n", stack[top]);
				stack[top] = carName--;
			}
			break;

		case 3:
			printf("μΆ…λ£\n");
			break;

		default:
			printf("λ‹¤μ‹ μ…λ ¥\n");
			break;
		}
	}

	return 0;
}



