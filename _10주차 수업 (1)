#include <stdio.h>
#include <string.h>

int main()
{
	int i, k;
	char str[100];

	printf("ë¬¸ìì—´ì„ ì…ë ¥: ");
	scanf("%s", str);

	k = strlen(str);

	for (i = 0; i < k; i++)
	{
		str[i] = str[(k - 1) - i];  
	}
	str[k] = '\0';

	printf("%s", str);
	
	return 0;
}

