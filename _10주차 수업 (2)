#include <stdio.h>
 
int main()
{
	char ch;
	char* p;
	int i;
	int k;

	ch = 'A';
	k = 65;
	p = &ch;
	i = &ch;

	printf("%d\n", ch);
	printf("%c\n", ch);
	printf("%d\n", k);
	printf("%c\n", k);
	printf("%d\n", p);
	printf("%d\n", i);
	printf("%d\n", *p);
	printf("%c\n", *p);

	return 0;
}		

